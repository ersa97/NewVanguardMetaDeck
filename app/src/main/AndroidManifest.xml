<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vanguardmetadeck">

    <application
        android:allowBackup="true"
        android:icon="@drawable/dicodingprofpic"
        android:label="@string/app_name"
        android:roundIcon="@drawable/dicodingprofpic"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Main.AboutActivity"/>
        <activity android:name=".Main.Detail.DetailedActivity" />

        <activity android:name=".SplashScreen.SplashScreenActivity"/>

        <activity android:name=".Main.MainActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>